{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"yarn run dev:electron\" \"yarn run dev:svelte\"",
    "dev:electron": "wait-on http://localhost:5173 && NODE_ENV=development electron .",
    "dev:svelte": "cd svelte-app && yarn dev"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^34.0.2",
    "wait-on": "^8.0.2"
  },
  "dependencies": {}
}
